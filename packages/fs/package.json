{
  "name": "@kra/fs",
  "version": "0.1.0",
  "exports": {
    ".": "./mod.ts",
    "./sync": "./sync.ts",
    "./async": "./async.ts",
    "./file": {
      "types": "./file/file.ts",
      "import": "./dist/file.mjs"
    },
    "./impl/dir": {
      "types": "./impl/types/dir.ts",
      "deno": "./impl/deno/dir.ts",
      "bun": "./impl/bun/dir.ts",
      "node": "./impl/node/dir.ts",
      "import": "./impl/stub/dir.ts"
    },
    "./impl/lstat": {
      "types": "./impl/types/lstat.ts",
      "deno": "./impl/deno/lstat.ts",
      "bun": "./impl/bun/lstat.ts",
      "node": "./impl/node/lstat.ts",
      "import": "./impl/stub/lstat.ts"
    }
  },
  "dkms": [{
    "name": "file",
    "watch": ["./file/**", "./tsdown.config.mts"],
    "script": "build:file"
  }],
  "scripts": {
    "build:file": "exec@tsdown"
  },
  "dependencies": {
    "quansync": "^0.2.10"
  },
  "devDependencies": {
    "tsdown": "^0.8.0",
    "unplugin-quansync": "^0.3.6"
  }
}
