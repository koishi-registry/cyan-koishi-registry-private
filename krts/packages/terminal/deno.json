{
  "name": "@krts/terminal",
  "version": "0.5.0-alpha.13",
  "exports": {
    "./lib": "./mod.ts",
    ".": "./client/index.ts",
    "./global": "./global.d.ts",
    "./deno.json": "./deno.json"
  },
  "imports": {
    "vite": "npm:rolldown-vite@latest",
    "@intlify/core-base": "npm:@intlify/core-base@^11",
    "sass-embedded": "npm:sass-embedded@^1"
  },
  "compilerOptions": {
    "types": ["@krts/terminal/global"],
    "lib": ["ES2022", "DOM"]
  },
  "lint": {
    "rules": {
      "exclude": [
        "no-namespace",
        "ban-types",
        "no-slow-types",
        "no-explicit-any",
        "require-await",
        "no-sloppy-imports",
        "no-empty-interface"
      ]
    }
  }
}
